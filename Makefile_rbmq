

flysim.out: Info.o receptor.o RandGen.o StiExt.o neuron_affi.o neuron_LIF3.o neuron_gp4.o neuronGnl.o EqsLIF3.o EqsLIF_gp4.o EqsGnl.o parser_affi.o parser.o rbmq.o NetSim.o NetSim_LIF3.o NetSimGnl.o NetSim_gp4.o NetSimLEGWorm.o main.o

	g++ Info.o receptor.o RandGen.o StiExt.o neuron_affi.o neuron_LIF3.o neuron_gp4.o neuronGnl.o EqsLIF3.o EqsLIF_gp4.o EqsGnl.o parser_affi.o parser.o rbmq.o NetSim.o NetSim_LIF3.o NetSimGnl.o NetSim_gp4.o NetSimLEGWorm.o main.o -o flysim.out -Wall -std=c++11 -pthread -rabbitmq -O3

main.o: main.cpp NetSim.h
	g++ -c main.cpp -Wall -std=c++11 -pthread -rabbitmq -O3

